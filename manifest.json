{
    "manifest_version": 3,
    "name": "Distraction Blocker",
    "version": "1.0",
    "description": "A Chrome extension that blocks social media so you can focus on your work.",
    "permissions": ["tabs", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_title": "Distraction Blocker"
    },
    "icons": {
      "128": "icons/icon128.png"
    }
  }